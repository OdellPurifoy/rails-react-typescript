<div class="columns">
  <div class="column">
    <h1 class="title">Users List</h1>
  </div>
</div>

<div class="columns">
  <div class="columns">
    <table>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td><td>
        <td><%= user.email %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>